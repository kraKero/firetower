{
    "name": "krakero/appman",
    "description": "This is my package appman",
    "keywords": [
        "krakero",
        "laravel",
        "appman"
    ],
    "homepage": "https://github.com/krakero/appman",
    "license": "MIT",
    "authors": [
        {
            "name": "KraKero LLC",
            "email": "vince@krakero.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "composer/composer": "^2.4",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "symfony/process": "^6.1"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Krakero\\Appman\\": "src",
            "Krakero\\Appman\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Krakero\\Appman\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Krakero\\Appman\\AppmanServiceProvider"
            ],
            "aliases": {
                "Appman": "Krakero\\Appman\\Facades\\Appman"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
